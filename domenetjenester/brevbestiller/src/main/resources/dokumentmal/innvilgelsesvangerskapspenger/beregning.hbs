{{#if beregningsgrunnlagRelevant}}
{{i18n "Inntekt_brukt"}}
    {{#if arbeidstakerEllerFrilanser}}
{{i18n "vi_har_brukt" bruttoBeregningsgrunnlag}}
    {{/if}}
    {{#if arbeidstaker}}
        {{#each arbeidsforhold}}
{{i18n "inntekten_din_AT" arbeidsgiverNavn manedsinntekt}}
        {{/each}}
        {{#gt arbeidsforhold.length 1}}
{{i18n "arbeidsgiverne_har_opplyst"}}
        {{else}}
{{i18n "arbeidsgiveren_har_opplyst"}}
        {{/gt}}
{{i18n "inntektgrunnlag_AT_ATSN"}}
    {{/if}}
    {{#if ikkeSoktForAlleArbeidsforhold}}
{{i18n "beregnet_svp_ut_fra_alle_arbeidsforholdene"}}
    {{/if}}
    {{#if frilanser}}
{{i18n "inntekten_din_FL" frilanser.manedsinntekt}}

        {{#unless arbeidstaker}}
{{i18n "inntektgrunnlag_FL"}}
        {{else}}
{{i18n "inntektgrunnlag_ATFL"}}
        {{/unless}}
    {{/if}}
{{#if ikkeSoktForAlleArbeidsforholdOgOppdrag}}
    {{i18n "beregnet_svp_ut_fra_alle_arbeidsforhold_og_oppdrag"}}
{{/if}}
    {{#if selvstendigNaringsdrivende}}
        {{#unless (or arbeidstaker frilanser)}}
            {{#with selvstendigNaringsdrivende}}
                {{#unless nyoppstartet}}
{{i18n "inntektgrunnlag_SN" aarsinntekt sistLignedeAar1 sistLignedeAar2 sistLignedeAar3}}
                {{else}}
{{i18n "inntektgrunnlag_ny_SN" aarsinntekt}}
                {{/unless}}
            {{/with}}
        {{else}}
            {{#if arbeidstaker}}
                {{#unless frilanser}}
{{i18n "inntektgrunnlag_SNAT" selvstendigNaringsdrivende.aarsinntekt}}

                    {{#if arbeidsinntektHoyereEnnSnittAvKombinertInntekt}}
{{i18n "AT_inntekt_gt_snitt_ATSN"}}
                    {{/if}}
                {{else}}
{{i18n "inntektgrunnlag_SNATFL" selvstendigNaringsdrivende.aarsinntekt}}

                    {{#if arbeidOgFrilansinntektHoyereEnnSnittAvKombinertInntekt}}
{{i18n "ATFN_inntekt_gt_snitt_ATFLSN"}}
                    {{/if}}
                {{/unless}}
            {{else}}
{{i18n "inntektgrunnlag_SNFL" selvstendigNaringsdrivende.aarsinntekt}}

                {{#if frilansinntektHoyereEnnSnittAvKombinertInntekt}}
{{i18n "FL_inntekt_gt_snitt_FLSN"}}
                {{/if}}
            {{/if}}
        {{/unless}}
        {{#if ikkeSoktForAlleArbeidsforholdOgNaringsvirksomhet}}
{{i18n "vi_har_beregnet_ut_fra_alle_arbeidsforhold_og_naering"}}

        {{/if}}
        {{#if ikkeSoktForAlleOppdragOgNaringsvirksomhet}}
{{i18n "vi_har_beregnet_ut_fra_alle_oppdrag_og_naering"}}

        {{/if}}
        {{#if ikkeSoktForAlleArbeidsforholdOppdragOgNaringsvirksomhet}}
{{i18n "vi_har_beregnet_ut_fra_alle_arbeidsforhold_oppdrag_og_naering"}}

        {{/if}}
    {{/if}}
    {{#if naturalYtelseUtbetalingEndretOpp}}
{{i18n "endring_naturalytelse_opp" naturalytelseEndringDato naturalytelseNyDagsats arbeidsgiverNavn}}

    {{/if}}
    {{#if naturalYtelseUtbetalingEndretNed}}
{{i18n "endring_naturalytelse_ned" naturalytelseEndringDato naturalytelseNyDagsats arbeidsgiverNavn}}

    {{/if}}
    {{#if militarSivil}}
{{i18n "militar_sivil"}}

    {{/if}}
    {{#if fritekst}}
        {{fritekst}}

    {{/if}}
    {{#if inntektOver6G}}
{{i18n "inntekt_over_seksG" seksG}}

    {{/if}}
{{i18n "lovref_beregning" lovHjemmelBeregning}}
{{/if}}
